<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--自定义组合控件-->
    <declare-styleable name="EditTextExpand">
        <!--自定义的属性名,类型-->
        <attr name="preLineColor" format="color"/> <!--下滑线的颜色-->
        <attr name="labelText" format="string"/> <!--标签内容-->
        <attr name="labelColor" format="color"/> <!--标签颜色-->
        <attr name="maxLength" format="integer"/> <!--输入内容最大限度-->
        <attr name="overlengthColor" format="color"/> <!--超出长度颜色-->
    </declare-styleable>
</resources>